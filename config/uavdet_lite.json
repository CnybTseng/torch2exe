// 这是算法模块的配置文件.
{
	// 承载算法负载的GPU设备号.
	// 如果有NUM_DEVICE个GPU设备, 此项取值范围为[0, NUM_DEVICE-1]内的整数.
	"device": 0,

	// 非最大值抑制阈值, 取值范围(0, 1). 值越大对重叠边框的抑制力越强.
	"nms_thresh": 0.4,
	
	// 物体判别的置信度阈值, 取值范围(0, 1). 值越小模型识别物体的灵敏度越高, 物体的误检率越高.
	// 值越大模型识别物体的灵敏度越低, 物体的漏检率越高.
	"score_thresh": 0.5,
	
	// 模型可以检测的物体类别. 类别列表(包括顺序)必须与模型严格匹配.
	"categories": [
		"person",
		"vehicle",
		"ship"
	],
	
	// 模型库文件路径
	"id_weight_path": "model/uavdet_lite.dll",
	
	// 启用半精度浮点数推理
	"use_fp16": false
}